compile:
	@javac -d ../classes ../src/State.java ../src/SwitchState.java ../src/Sensor.java ../src/MagneticSensor.java ../src/Door.java ../src/Window.java
	@javac -d ../classes -cp ../classes Stage1.java
	@echo Compilaci√≥n completada
run:
	@java -cp ../classes Stage1 config.txt
clean:
	@rm -rf ../classes
	@echo Archivos eliminados